{
  "name": "warp10-download",
  "version": "0.0.1",
  "description": "Warp 10 download badge",
  "module": "dist/esm/index.js",
  "main": "dist/index.js",
  "types": "dist/types/components.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "stencil build",
    "start": "stencil build --dev --watch --serve",
    "test": "jest",
    "test.watch": "jest --watch"
  },
  "dependencies": {
    "whatwg-fetch": "^2.0.4"
  },
  "devDependencies": {
    "@stencil/core": "^0.10.0",
    "@stencil/sass": "0.0.6",
    "@stencil/utils": "0.0.4",
    "@types/jest": "^21.1.1",
    "jest": "^21.2.1"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:Giwi/warp10-download.git"
  },
  "author": "Xavier MARIN",
  "license": "Apache-2",
  "bugs": {
    "url": "https://github.com/Giwi/warp10-download/issues"
  },
  "homepage": "https://github.com/Giwi/warp10-download",
  "jest": {
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/node_modules/@stencil/core/testing/jest.preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(tsx?|jsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json",
      "jsx"
    ]
  }
}
